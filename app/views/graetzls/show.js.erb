(function() {
  var $streamContainer = $("[data-behavior='stream-container']");
  var $paginateLink = $("[data-behavior='paginate-link']");

  function updateComponent() {
    $streamContainer.append("<%= j render @activity %>");
    $paginateLink.replaceWith("<%= j view_more_link @activity %>");
    APP.components.masonryFilterGrid.adjustNewCards();
  }

  updateComponent();
})();
