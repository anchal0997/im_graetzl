(function() {
  var $container;
  var $paginateLink;

  function updateComponent() {
    <% if @posts.present? %>
      $container = $("[data-behavior='stream-container']");
      $paginateLink = $("[data-behavior='paginate-link']");
      $container.append("<%= j render partial: 'location_posts/stream_post', collection: @posts, as: 'post' %>");
      $paginateLink.replaceWith("<%= j view_more_link @posts %>");
    <% elsif @meetings.present? %>
      $container = $("[data-behavior='meetings-container']");
      $paginateLink = $("[data-behavior='paginate-meetings']");
      $container.append("<%= j render @meetings %>");
      $paginateLink.replaceWith("<%= j view_more_of @meetings, 'meetings' %>");
    <% end %>
  }
  updateComponent();
})();
