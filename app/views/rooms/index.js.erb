(function() {
  var $cardContainer = $("[data-behavior='rooms-card-container']");
  var $paginateLink = $("[data-behavior='rooms-paginate-link']");

  <% if params[:page].blank? %>
    <% if @rooms.empty? %>
      $cardContainer.html("<%= j render 'empty_message' %>");
    <% else %>
      $cardContainer.find(".cardBox").remove();
      $cardContainer.append("<%= j render @rooms %>");
    <% end %>
  <% else %>
    $cardContainer.append("<%= j render @rooms %>");
  <% end %>

  <% if @next_page %>
    $paginateLink.attr('href', "<%= j next_page_url %>").show();
  <% else %>
    $paginateLink.hide();
  <% end %>
})();
