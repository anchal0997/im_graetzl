      $(document).ready(function() {

          APP.components.imgUploadPreview();

          $(".respondForm textarea, .writeEntry textarea").autogrow();

          $(".respondForm textarea")
                  .on("focus", function() {
                      var $parent = $(this).parents(".respondForm");
                      $parent.addClass("is-focused");
                  })
                  .on("blur", function() {
                      var $parent = $(this).parents(".respondForm");
                      if(!$(this).val().length) {
                          $parent.removeClass("is-focused");
                      }
                  });

          $(".writeEntry textarea")
                  .on("focus", function() {
                      var $parent = $(this).parents(".writeEntry");
                      $parent.addClass("is-focused");
                  })
                  .on("blur", function() {
                      var $parent = $(this).parents(".writeEntry");
                      if(!$(this).val().length) {
                          $parent.removeClass("is-focused");
                      }
                  });

