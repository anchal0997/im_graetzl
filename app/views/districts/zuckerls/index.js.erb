(function() {
  var $streamContainer = $("[data-behavior='stream-container']");
  var $paginateLink = $("[data-behavior='paginate-link']");

  function updateComponent() {
    $streamContainer.append("<%= j render partial: 'zuckerls/zuckerl', collection: @zuckerls %>");
    $paginateLink.replaceWith("<%= j view_more_link @zuckerls, style: :big %>");
  }

  updateComponent();
})();
